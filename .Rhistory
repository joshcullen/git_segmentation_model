heatmap=function(data, brkpts, dat.res, type) {  #type can either be 'loc' or 'behav'
if (type == "loc") {
par(ask = TRUE)
map(data, ~plot.heatmap.loc(., brkpts = brkpts, dat.res = dat.res))
par(ask = FALSE)
} else if (type == "behav") {
par(ask = TRUE)
